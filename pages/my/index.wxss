/**my.wxss**/
.Head{padding:10px 15px;background: #fff;border-bottom: 1px solid #e5e5e5;overflow: hidden;}
.Head-portrait image{width: 80px;height: 80px;float: left;}
.Head-info{padding-left:90px;}
.Head-info view{line-height: 25px;font-size:15px;}
.Head-info .title{font-size: 16px;}
.Head-info .boss{font-size:14px;}
.Head-info .cash{font-size:14px;}
.cash text{color: #F13D3C;}
.my-list{margin: 10px 0 ;border-top: 1px solid #e5e5e5;}
.list{padding:0 15px;overflow: hidden;border-bottom: 1px solid #e5e5e5;line-height: 44px;background: #fff;}
.Arrow{float: right}
.list-title{float: left;font-size:15px;}
.spacing{margin-top:10px;border-top:1px solid #e5e5e5;}
navigator {display: block;}
.Arrow image{width:20px;height:20px;display: block;margin-top:10px;}
.oil_fund{padding-right:10px;}
.public-bg{padding-bottom: 50px;}
.icon{float:left;width:22px;height:22px;margin-top:11px;margin-right:15px;}